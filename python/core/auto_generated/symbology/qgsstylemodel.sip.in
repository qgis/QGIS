      TypeRole,
      TagRole,
      EntityName,
      SymbolTypeRole,
      IsFavoriteRole,
      LayerTypeRole,
      CompatibleGeometryTypesRole,
      StyleName,
      StyleFileName,
      IsTitleRole,
    };

