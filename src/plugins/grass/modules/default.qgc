                <grass name="r.report"/>
                <grass name="r.average" version_max="6.4"/>
                <grass name="r.median" version_max="6.4"/>
                <grass name="r.mode"/>
                <grass name="r.volume"/>
                <grass name="r.surf.area"/>
                <grass name="r.univar"/>
                <grass name="r.covar"/>
                <grass name="r.regression.line"/>
                <grass name="r.coin"/>
                <grass name="r.distance"/>
