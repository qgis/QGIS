                    <grass name="r.in.ascii"/>
                    <grass name="r.in.poly"/>
                    <grass name="r.in.bin"/>
                    <grass name="r.in.arc" version_max="6.4"/>
                    <grass name="r.in.gridatb"/>
                    <grass name="r.in.mat"/>
