                <grass name="r.out.ascii"/>
                <grass name="r.out.xyz"/>
                <grass name="r.out.arc" version_max="6.4"/>
                <grass name="r.out.gridatb"/>
                <grass name="r.out.mat"/>
                <grass name="r.out.bin"/>
