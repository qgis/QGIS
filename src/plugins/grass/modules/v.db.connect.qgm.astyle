<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE qgisgrassmodule SYSTEM "http://mrcc.com/qgisgrassmodule.dtd">

<qgisgrassmodule label="Connect vector to database" module="v.db.connect">
	<option key="map" layeroption="layer"/>
	<option key="driver" answer="dbf" hidden="no"/>
	<option key="database" answer="$GISDBASE/$LOCATION_NAME/$MAPSET/dbf/" hidden="no"/>
	<option key="table"/>
	<field key="key"/>
	<flag key="o" answer="on" hidden="no"/>
</qgisgrassmodule>
