{
  "vcpkg-configuration": {
    "default-registry": {
      "kind": "git",
      "baseline": "5d57f5a0a5469a23e005fc79a7c1814ab4fc967e",
      "repository": "https://github.com/microsoft/vcpkg"
    },
    "registries": [
      {
        "kind": "git",
        "baseline": "c8ff38b5c7e84ed2380f14cffb137bf76f9d6418",
        "repository": "https://github.com/open-vcpkg/python-registry",
        "packages": [
          "python3",
          "py-*",
          "gdal",
          "vcpkg-python-scripts",
          "vcpkg-tool-mercurial",
          "vcpkg-tool-rust",
          "pybind11"
        ]
      }
    ],
    "overlay-ports": [
      "ports"
    ],
    "overlay-triplets": [
      "triplets"
    ]
  },
  "name": "qgis",
  "version-string": "current",
  "description": "QGIS is a free, open source, cross platform (lin/win/mac) geographical information system (GIS)",
  "homepage": "https://qgis.org",
  "dependencies": [

    "expat",
    {
      "name": "gdal",
      "default-features": false,
      "features": [
        "freexl"
      ]
    },
    "geos",
    "gsl",
    "jhasse-poly2tri",
    "libspatialindex",
    {
      "name": "libtasn1",
      "platform": "ios | osx"
    },
    "libxml2",
    "libzip",
    "meshoptimizer",
    "nlohmann-json",
    "o2",
    "proj",
    "protobuf",
    {
      "name": "qtbase",
      "default-features": false,
      "features": [
        "opengl",
        {
          "name": "gles3",
          "platform": "emscripten"
        },
        "concurrent",
        "freetype",
        "gui",
        "jpeg",
        "network",
        "widgets",
        "sql-sqlite",
        "testlib"
      ]
    },
    {
      "name": "qtbase",
      "default-features": false,
      "host": true
    },
    "qtsvg",
    "qtmultimedia",
    "qt5compat",
    "qtkeychain-qt6",
    "zlib",
    "zstd"
  ],
  "default-features": [
    "recommended-features"
  ],
  "features": {
    "3d": {
      "description": "Determines whether QGIS 3D library should be built",
      "dependencies": [
        "qt3d"
      ]
    },
    "auth": {
      "description": "Build with Auth (QCA) support",
      "dependencies": [
        {
          "name": "qca",
          "default-features": false,
          "features": [
            "ossl"
          ]
        }
      ]
    },
    "bindings": {
      "description": "Determines whether Python bindings should be built",
      "dependencies": [
        {
          "name": "arrow",
          "features": [
            "python"
          ]
        },
        {
          "name": "gdal",
          "features": [
            "python"
          ]
        },
        "py-adbc-postgresql",
        "py-adbc-sqlite",
        "py-attrs",
        "py-autopep8",
        "py-beautifulsoup4",
        "py-dateutil",
        "py-duckdb",
        {
          "name": "py-geopandas",
          "features": [
            "fiona",
            "arrow"
          ]
        },
        "py-scipy",
        "py-isort",
        "py-jinja2",
        {
          "name": "py-jsonschema",
          "platform": "!windows"
        },
        "py-lxml",
        "py-markupsafe",
        "py-networkx",
        "py-numpy",
        "py-owslib",
        "py-pip",
        "py-psycopg",
        "py-psycopg2",
        "py-pydantic",
        "py-pyproj",
        {
          "name": "py-pyqt6",
          "features": [
            "qt3d",
            "qt5compat",
            "qtcharts",
            "qtdeclarative",
            "qtpositioning",
            "qtserialport"
          ]
        },
        "py-pysal",
        "py-pytz",
        "py-pyyaml",
        "py-qscintilla",
        "py-rasterio",
        "py-requests",
        "py-shapely",
        "py-six",
        "py-urllib3",
        "python3"
      ]
    },
    "recommended-features": {
      "description": "Enable a recommended QGIS build",
      "dependencies": [
        "duckdb",
        "libpq",
        "libspatialite",
        {
          "name": "gdal",
          "default-features": true,
          "features": [
            "arrow-adbc",
            "kea",
            "parquet",
            "poppler",
            "tools"
          ]
        },
        {
          "name": "qtbase",
          "features": [
            "sql-odbc"
          ]
        },
        "opencl",
        "qtdeclarative",
        "qtimageformats",
        "qtquickcontrols2",
        "qttools",
        "qtpositioning"
      ]
    },
    "exiv2": {
      "description": "Determines whether EXIV2 should be built",
      "dependencies": [
        {
          "name": "exiv2",
          "features": [
            "xmp"
          ]
        }
      ]
    },
    "gui": {
      "description": "Determines whether QGIS gui should be built",
      "dependencies": [
        "qscintilla",
        "qtmultimedia",
        "qwt"
      ]
    },
    "opencl": {
      "description": "Should OpenCL support be added",
      "dependencies": [
        "opencl"
      ]
    },
    "oracle": {
      "description": "Build with oracle support",
      "dependencies": [
        "oracle-instantclient"
      ]
    },
    "pdal": {
      "description": "Build with PDAL support",
      "dependencies": [
        "pdal"
      ]
    },
    "qtpositioning": {
      "description": "Build with QtPositioning support",
      "dependencies": [
        "qtpositioning"
      ]
    },
    "server": {
      "description": "Determines whether QGIS server should be built",
      "dependencies": [
        "fastcgi"
      ]
    },
    "sfcgal": {
      "description": "Build with SFCGAL support",
      "dependencies": [
        "sfcgal"
      ]
    }
  }
}
